{
    "name": "jobins/laravel-repository",
    "description": "A basic light weight package to implement Repository Design Pattern in Laravel.",
    "keywords": [
        "jobins",
        "laravel",
        "laravel-repository"
    ],
    "homepage": "https://github.com/jobins/laravel-repository",
    "license": "MIT",
    "authors": [
        {
            "name": "Puncoz Nepal",
            "email": "puncoz138@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "nunomaduro/larastan": "^1.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "JoBins\\LaravelRepository\\": "src"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "JoBins\\LaravelRepository\\LaravelRepositoryServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
